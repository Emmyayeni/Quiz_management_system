.notification--coupon { max-width: 480px; padding: 30px; margin: 0px 0px 20px; }

.notification--coupon .notification__close { min-width: 54px; height: 54px; font-size: 18px; line-height: 54px; }

.notification--coupon .notification__close i { font-size: 24px; }

.notification--coupon .notification__close:hover { color: inherit !important; }

.notification--coupon .content { cursor: pointer; }

.notification--coupon .coupon-img i, .notification--coupon .coupon-img img { margin-bottom: 20px; }

.notification--coupon .group__context { padding: 0px 15px; border-color: rgb(227, 233, 237); }

.notification--coupon .group__context span { font-size: 14px; }

.notification--coupon .group [class*="button"] { font-size: 14px; line-height: 24px; border-top-right-radius: 3px; border-bottom-right-radius: 3px; }

.notification--coupon .group button { padding: 0px 10px; height: 38px; }

@media screen and (max-width: 480px) {
  .notification--coupon { padding: 20px 30px; }
}

#adobe-coupon-popup { cursor: pointer; }

#adobe-coupon .countdown__description { display: none !important; }

#adobe-coupon ul { font-size: 20px; display: flex; align-items: center; justify-content: center; margin: 0px 0px 20px; font-weight: 600; }

@media screen and (min-width: 480px) {
  #adobe-coupon ul { font-size: calc(20px + (6 * (100vw - 480px)) / 920); }
}

@media screen and (min-width: 1400px) {
  #adobe-coupon ul { font-size: 26px; }
}

#adobe-coupon li:not(:last-child) .countdown__value::after { content: ":"; }

#adobe-coupon { position: relative; min-height: 270px; max-width: 480px; background-position: center center; background-size: cover; }

#adobe-coupon a, #adobe-coupon img { position: absolute; top: 50%; left: 50%; width: 100%; height: 100%; transform: translate(-50%, -50%); }

#adobe-coupon img { height: auto; }

@supports (object-fit: cover) {
  #adobe-coupon img { height: 100%; object-fit: cover; }
}

#adobe-coupon .notification__close { z-index: 10; }

#adobe-coupon .notification__close.dark-close { color: rgb(29, 38, 45); }

.coupon-80 { background: linear-gradient(118.89deg, rgb(255, 255, 255) 40%, rgb(199, 235, 255) 100%); overflow: hidden; }

.coupon-80 .content { display: block; max-width: 222px; }

.coupon-80 .coupon-img i { font-size: 20px; color: rgb(18, 115, 235); }

.coupon-80 .coupon-text .coupon-title { font-size: 18px; font-weight: 700; color: rgb(18, 115, 235); display: block; margin-bottom: 10px; }

.coupon-80 .coupon-text p { font-weight: 600; color: rgb(20, 59, 99); max-width: 215px; margin: 0px 0px 25px; }

.coupon-80 .coupon-text p span { font-weight: 700; color: rgb(18, 115, 235); }

.coupon-80 picture { position: absolute; pointer-events: none; }

.coupon-80 picture.bg { top: 0px; right: -50px; width: 65%; height: 100%; }

.coupon-80 picture img { width: 100%; height: 100%; object-fit: cover; }

@media screen and (max-width: 480px) {
  .coupon-80 .notification__content { padding-right: 0px; }
  .coupon-80 .bg { display: none; }
}

.form-alert { display: flex; align-items: baseline; width: 100%; font-size: 13px; font-weight: 600; }

.form-alert i { position: relative; top: 4px; margin-right: 10px; }

.form-alert--background { border-radius: 8px; padding: 10px; margin-bottom: 30px; }

.form-alert--background.form-alert--error, .form-alert--background.form-alert--error i { color: rgb(213, 50, 50); background-color: rgb(255, 222, 204); }

.form-alert--background.form-alert--success { background-color: rgba(162, 247, 197, 0.4); }

.form-alert--background.form-alert--success, .form-alert--background.form-alert--success i { color: rgb(0, 157, 115); }

.form-alert:not(.form-alert--background).form-alert--error { color: rgb(236, 58, 59); }

