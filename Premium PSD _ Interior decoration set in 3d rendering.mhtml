.list li { position: relative; margin: 0px 0px 2.5px; padding: 0px 0px 0px 10px; list-style: none; }

.list li::before { display: inline-block; width: 6px; height: 6px; margin: 9px 10px 10px -10px; border-radius: 50%; background-color: rgb(55, 73, 87); vertical-align: middle; content: ""; }

.list--xs li { font-size: 13px; line-height: 1.66; }

.list--xs li::before { margin: 7.5px 5px 0px 0px; }

.list--sm li { font-size: 14px; line-height: 1.66; }

.list--sm li::before { margin: 8.5px 5px 0px 0px; }

.list--md li { font-size: 16px; line-height: 1.66; }

.list--md li::before { margin: 10px 5px 0px 0px; }

.list--lg li { font-size: 18px; line-height: 1.66; }

.list--lg li::before { margin: 11.5px 5px 0px 0px; }

.list--xl li { font-size: 22px; line-height: 1.66; }

.list--xl li::before { margin: 15px 5px 0px 0px; }

.list--horizontal { display: block; }

.list--horizontal::after { clear: both; content: ""; display: table; }

.list--horizontal li { float: left; margin: 0px 10px 0px 0px; }

.list--horizontal li:last-child { margin: 0px; }

.list--comma-separated li { margin: 0px; padding: 0px; }

.list--comma-separated li:not(:last-child)::after { content: ", "; }

.list--comma-separated li:last-child::after { content: "."; }

